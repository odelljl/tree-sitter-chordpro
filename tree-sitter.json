{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "chordpro",
      "camelcase": "ChordPro",
      "title": "ChordPro",
      "scope": "source.chordpro",
      "file-types": [
        "chord",
        "chopro",
        "cho",
        "pro",
        "crd"
      ],
      "injection-regex": "^chordpro$",
      "class-name": "TreeSitterChordpro"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "This grammar supports the mark-up for the ChordPro file format.",
    "authors": [
      {
        "name": "Jeff Odell",
        "email": "jeff@odellmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/odelljl/tree-sitter-chordpro.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}